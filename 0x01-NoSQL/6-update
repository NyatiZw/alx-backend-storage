//Script that adds new attribute to collection
#!/usr/bin/bash

if [ "$#" -ne 1 ]; then

    echo "Usage: $0 ,database_name>"
    exit 1
fi

db_name="$1"

mongo "db_name" --quiet --eval 'db.school.updateMany({ "name": "Holberton school" }, { $set: { "address": "972 Mission street" } })'

echo "bye"
